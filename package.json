{
  "name": "coffee-loader",
  "version": "0.7.3",
  "author": "Tobias Koppers @sokra",
  "description": "coffee loader for webpack",
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "peerDependencies": {
    "coffeescript": ">= 1.8.x",
    "webpack": " >= 2.x"
  },
  "repository": {
    "type": "git",
    "url": "webpack-contrib/coffee-loader.git"
  },
  "licenses": "MIT",
  "scripts": {
    "webpack-defaults": "webpack-defaults",
    "start": "yarn run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "clean": "del-cli dist test/builds",
    "docs": "jsdoc2md src/index.js > LOADER.md",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "yarn run clean",
    "prepublish": "yarn run build",
    "release": "yarn run standard-version",
    "security": "nsp check",
    "pretest": "yarn run build && node test/webpack.build.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:coverage": "yarn run test:coverage",
    "travis:lint": "yarn run lint && yarn run security",
    "travis:test": "yarn run test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "cross-env": "^4.0.0",
    "del-cli": "^0.2.1",
    "eslint": "^3.19.0",
    "eslint-config-webpack": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "fs-extra": "^2.1.2",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^3.4.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.0.0",
    "webpack": "^2.3.3",
    "webpack-defaults": "^0.4.5"
  },
  "engines": {
    "node": ">= 4.3 < 5.0.0 || >= 5.10"
  },
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
